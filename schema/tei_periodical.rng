   <define name="OpenArabicPE_body">
      <element name="body">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. Default Text Structure]</a:documentation>
         <group>
      
      
            <zeroOrMore>
               <ref name="OpenArabicPE_model.global"/>
            </zeroOrMore>
      
      
      
            <optional>
               <group>
          
                  <ref name="OpenArabicPE_model.divTop"/>
          
          
                  <zeroOrMore>
                     <choice>
                        <ref name="OpenArabicPE_model.global"/>
                        <ref name="OpenArabicPE_model.divTop"/>
                     </choice>
                  </zeroOrMore>
          
               </group>
            </optional>
      
      
      
            <optional>
               <group>
          
                  <ref name="OpenArabicPE_model.divGenLike"/>
          
          
                  <zeroOrMore>
                     <choice>
                        <ref name="OpenArabicPE_model.global"/>
                        <ref name="OpenArabicPE_model.divGenLike"/>
                     </choice>
                  </zeroOrMore>
          
               </group>
            </optional>
      
      
        
            <choice>
          
          
               <oneOrMore>
                  <group>
              
                     <ref name="OpenArabicPE_model.divLike"/>
              
              
                     <zeroOrMore>
                        <choice>
                           <ref name="OpenArabicPE_model.global"/>
                           <ref name="OpenArabicPE_model.divGenLike"/>
                        </choice>
                     </zeroOrMore>
              
                  </group>
               </oneOrMore>
          
          
          
               <oneOrMore>
                  <group>
              
                     <ref name="OpenArabicPE_model.div1Like"/>
              
              
                     <zeroOrMore>
                        <choice>
                           <ref name="OpenArabicPE_model.global"/>
                           <ref name="OpenArabicPE_model.divGenLike"/>
                        </choice>
                     </zeroOrMore>
              
                  </group>
               </oneOrMore>
          
          
               <group>
                  <oneOrMore>
                     <group>
              
                        <ref name="OpenArabicPE_model.common"/>
              
              
                        <zeroOrMore>
                           <ref name="OpenArabicPE_model.global"/>
                        </zeroOrMore>
              
                     </group>
