                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Two digits</a:documentation>
                  <value>d3</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Three digits</a:documentation>
                  <value>d4</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Four digits</a:documentation>
                  <value>d5</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Five digits</a:documentation>
                  <value>d6</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Six digits</a:documentation>
                  <data type="Name"/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">indicates the type of numeric value.
